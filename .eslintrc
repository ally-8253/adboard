{
    "env": {
      "browser": true,
      "jest": true,
      "es6": true
    },
    "plugins": ["import", "react"],
    "extends": ["eslint:recommended", "airbnb", "plugin:prettier/recommended"],
    "parserOptions": {
			"ecmaVersion": 2018,
			"ecmaFeatures": {
			"jsx": true
			}
    },
    "rules": {
      "no-console": "warn",
      "no-eval": "error",
      "import/first": "error"
    }
  }